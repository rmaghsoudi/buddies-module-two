<%= form_for @study_session do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>

<%= f.label :tutor_id %>
<%= f.collection_select(:tutor_id, User.tutors, :id, :first_name, prompt: true) %><br>

<%= f.label :student_id %>
<%= f.collection_select(:student_id, User.students, :id, :first_name, prompt: true) %><br>

<%= f.label :subject_id %>
<%= f.collection_select(:subject_id, Subject.all, :id, :name, prompt: true) %><br>

<%= f.label :date %>

<%= datetime_local_field(:study_session, :date) %><br>

<%= f.submit%>
<%end%>
